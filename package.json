{
  "name": "rlab-as-a-service",
  "version": "1.0.0",
  "description": "Code Network Winter Hack 2018 Brisbane",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch",
    "build-client": "node ./node_modules/webpack/bin/webpack --config ./client/webpack.config.js --watch --progress --profile --colors",
    "build-server": "tsc -p ./",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Server,Client,Nodemon\" -c \"cyan.bold,magenta.bold,green.bold\" \"tsc -w -p ./\" \"npm run build-client\" \"npm run serve\"",
    "serve": "nodemon server/dist/index.js"
  },
