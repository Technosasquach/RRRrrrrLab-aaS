  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/connect-mongo": "0.0.38",
    "@types/cookie-parser": "^1.4.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/lusca": "^1.5.0",
